{
  "conventionalCommits.scopes": ["releases", "dx"],
  "editor": {
    "bracketPairColorization.enabled": true,
    "tabSize": 2
  },
  "emmet.includeLanguages": {
    "vue-html": "html",
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "files.associations": {
    "*.css": "postcss"
  },
  "jest.jestCommandLine": "./node_modules/.bin/jest"
}
